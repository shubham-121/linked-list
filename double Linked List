#include <iostream>

using namespace std;

class Node{
    public:
    int data;
    Node* next;
    Node* prev;
    
    
    Node(int data)
    {
        this->data=data;
        this->next=NULL;
        this->prev=NULL;
    }
};

void insertAtHead(Node* &head, int x)
{
   Node* temp=new Node(x);
   
   //Node* temp=head;
   
   temp->next=head;
   head->prev=temp;
   head=temp;
}

void insertAtTail(Node* &tail, int x)
{
    Node* temp=new Node(x);
    tail->next=temp;
    temp->prev=tail;
    tail=temp;
}

void print(Node* &head)
{
    Node* temp=head;
    
    while(temp!=NULL)
    {
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    cout<<endl;
}


int main()
{
    Node* newNode=new Node(10);
    Node* head=newNode;
    Node* tail=newNode;
    
    print(head);
    
    insertAtHead(head,20);
    insertAtHead(head,30);
    
    insertAtTail(tail, 50);
    print(head);
   
    
    return 0;
}
